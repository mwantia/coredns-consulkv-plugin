{
    "name": "CoreDNS ConsulKV Plugin Development",
    "dockerComposeFile": "compose.yml",
    "service": "coredns-dev",
    "workspaceFolder": "/app/coredns-consulkv-plugin",
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go",
                "ms-azuretools.vscode-docker"
            ]
        }
    },
    "forwardPorts": [5301, 8501],
    "postCreateCommand": "go mod download",
    "remoteUser": "root"
}